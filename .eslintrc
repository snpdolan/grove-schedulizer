{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true,
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "plugins": [
    "react",
    "jsx"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}